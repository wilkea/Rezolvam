@model ProfileViewModel

<h2>Profilul meu</h2>

<div>
    <p><strong>FullName:</strong> @Model.FullName</p>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Roles:</strong></p>
    <ul>
        @foreach (var role in Model.Roles)
        {
            <li>@role</li>
        }
    </ul>
</div>
